
<%= error_messages_for :chronicle %>

<%= form_for @chronicle do |f| -%>
  <label for="chronicle_name">Name:&nbsp;</label>
  <%= f.text_field :name, :size => 60 %>

  <div id="chronicle_examples">
    <%= "Example: \"#{@example_names}\"" %>
  </div>

  <p>
    <%= submit_tag 'Create' %>
    <%= nbsp(2) %>
    <%= link_to 'Cancel', chronicles_path %>
  </p>
<% end -%>

